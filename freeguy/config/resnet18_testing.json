{   

    "image_size": 256,
    "epochs": 50,
    "root": "/media/nishanth/Nishanth M/Dataset/sub_imagenet/imagenet-mini/", 
    "dataloader_name":"imagenet",
    "dataloader":{
        "transforms": {
            "name": "resnet_imagenet_transforms"
        },
        "train": {
            "batch_size": 16,
            "shuffle": true,
            "num_workers": 4,
            "pin_memory": true,
            "drop_last": false,
            "persistent_workers": true
        },
        "val": {
            "batch_size": 16,
            "shuffle": false,
            "num_workers": 4,
            "pin_memory": true,
            "drop_last": false,
            "persistent_workers": true
        }
    },
    "loss":{
        "name": "CrossEntropyLoss"
    },
    "optimizer" : {
        "name": "sgd",
        "lr": 0.00005,
        "momentum": 0.9,
        "weight_decay": 0.00005
    },
    "scheduler" : {
        "name": "CosineAnnealingWarmRestarts",
        "first_cycle_steps": 200,
        "cycle_mult": 1.0,
        "max_lr": 0.1,
        "min_lr": 0.001,
        "warmup_steps": 50,
        "gamma": 0.5
    },
    "summary":{
        "name": "resnet18_baseline_002",
        "log_dir":"./runs"
    }
}